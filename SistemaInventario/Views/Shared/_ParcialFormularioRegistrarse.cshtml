@using DAL.Models;
@model tabClientes
<label>Nombre Cliente: </label>
@Html.TextBoxFor(x => x.nombre, htmlAttributes: new { @class = "form-control" })
@Html.ValidationMessageFor(x => x.nombre, null, htmlAttributes: new { @class = "text-danger" })
<label>Teléfono </label>
@Html.TextBoxFor(x => x.telefono, new { @class = "form-control" })
@Html.ValidationMessageFor(x => x.telefono, null, htmlAttributes: new { @class = "text-danger" })
@Html.ValidationMessage("Numero", new { @class = "text-danger" })
<label>Dirección: </label>
@Html.TextBoxFor(x => x.direccion, new { @class = "form-control" })
@Html.ValidationMessageFor(x => x.direccion, null, htmlAttributes: new { @class = "text-danger" })
<label>Correo: </label>
@Html.TextBoxFor(x => x.correo, new { @class = "form-control" })
@Html.ValidationMessageFor(x => x.correo, null, htmlAttributes: new { @class = "text-danger" })
@Html.ValidationMessage("CorreoExiste", new { @class = "text-danger" })
<label>Contraseña: </label>
@Html.PasswordFor(x => x.pass, new { @class = "form-control" })
@Html.ValidationMessageFor(x => x.pass, null, htmlAttributes: new { @class = "text-danger" })
<label>Confirmar Contraseña: </label>
@Html.PasswordFor(x => x.ConfirmarPass, new { @class = "form-control" })
@Html.ValidationMessageFor(x => x.ConfirmarPass, null, htmlAttributes: new { @class = "text-danger" })
