@{
    ViewBag.Title = "Home Page";
}
<link href="~/Content/Diseños/index.css" rel="stylesheet" />

@if (TempData["Message"] != null)
{     <center>
    <div class="alert alert-success alert-dismissable col-md-4 text-center" role="alert" style="margin-top: 10px; width: 35rem; color: black">
        <button type="button" class="close" data-dismiss="alert">&times;</button>
        <b>@TempData["Message"]</b>
    </div>
</center>
}

<div class="Contenedor">
    <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
        <div class="carousel-inner caret">
            <div class="carousel-item active">
                <img class="imgCarrusel d-block " src="~/Content/img_cars/4.jpg" alt="First slide">
            </div>
            <div class="imgCarrusel carousel-item">
                <img class="d-block " src="~/Content/img_cars/1.jpg" alt="Third slide">
            </div>
            <div class="imgCarrusel carousel-item">
                <img class="d-block " src="~/Content/img_cars/3.jpg" alt="Third slide">
            </div>
            <div class="imgCarrusel carousel-item">
                <img class="d-block " src="~/Content/img_cars/car4.jpg" alt="Third slide">
            </div>
            <div class="imgCarrusel carousel-item">
                <img class="d-block " src="~/Content/img_cars/car5.jpg" alt="Third slide">
            </div>
            <div class="imgCarrusel carousel-item">
                <img class="d-block " src="~/Content/img_cars/white-black-car.jpg" alt="Third slide">
            </div>
            <div class="imgCarrusel carousel-item">
                <img class="d-block " src="~/Content/img_cars/yellow-black-car.jpg" alt="Third slide">
            </div>

        </div>
        <a class="carousel-control-prev" href="#carouselExampleControls" role="button" data-slide="prev">
            <span class="carousel-control-prev-icon" aria-hidden="true"></span>
            <span class="sr-only">Previous</span>
        </a>
        <a class="carousel-control-next" href="#carouselExampleControls" role="button" data-slide="next">
            <span class="carousel-control-next-icon" aria-hidden="true"></span>
            <span class="sr-only">Next</span>
        </a>
    </div>
</div>
<div class="Contenedor">
    <div class="row">
    </div>
</div>
