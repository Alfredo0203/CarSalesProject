@using DAL.Models
@model List<tabInventario>
@{
    ViewBag.Title = "MostrarAutos";
}

<div class="container">
    <div class="row">
        <div class="col-md-12">
            <br />
            <center><h1>Listado de Inventario</h1></center>
            <br />
            
            <table class="table-hover table-striped ">
                <thead>
                    <tr>
                        <th>Marca</th>
                        <th>Modelo</th>
                        <th>Precio Venta</th>
                        <th>Imagen</th>
                        <th>Fecha registro</th>
                    </tr>
                </thead>
                <tbody>
                    @if (Model != null)
                    {
                        foreach (var car in Model)
                        {
                            <tr>
                                <td>@car.MarcaAuto</td>
                                <td>@car.ModeloAuto</td>
                                <td><b>$</b>@car.PrecioAuto</td>
                                <td>@car.Imagen</td>
                                <td>@car.Fecha</td>
                               
                            </tr>
                        }
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>